    "@types/history": "^4.7.9",
    "@types/js-cookie": "^2.2.7",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/mime-types": "^2.1.0",
    "@types/mini-css-extract-plugin": "2.0.1",
